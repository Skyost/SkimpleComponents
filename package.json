{
  "name": "skimple-components",
  "version": "0.5.0",
  "type": "module",
  "description": "SkimpleComponents is my personal components library built on Bootstrap and designed to be used with Nuxt.",
  "homepage": "https://github.com/Skyost/SkimpleComponents",
  "bugs": {
    "url": "https://github.com/Skyost/SkimpleComponents/issues"
  },
  "license": "MIT",
  "author": {
    "name": "Skyost",
    "url": "https://skyost.eu"
  },
  "repository": "Skyost/SkimpleComponents",
  "keywords": [
    "vue",
    "vuejs",
    "components",
    "bootstrap",
    "skimple",
    "nuxt",
    "nuxtjs",
    "nuxt.js"
  ],
  "main": "./dist/skimple-components.umd.cjs",
  "unpkg": "./dist/skimple-components.js",
  "jsdelivr": "./dist/skimple-components.js",
  "module": "./dist/skimple-components.js",
  "exports": {
    ".": {
      "import": "./dist/skimple-components.js",
      "require": "./dist/skimple-components.umd.cjs"
    }
  },
  "files": [
    "dist/*.js",
    "dist/*.js.map",
    "lib/style.scss",
    "nuxt"
  ],
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check build-only",
    "preview": "vite preview --port 4173",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.1.0",
    "@types/node": "^16.11.45",
    "@vitejs/plugin-vue": "^3.0.1",
    "@vue/eslint-config-typescript": "^11.0.0",
    "@vue/tsconfig": "^0.1.3",
    "eslint": "^8.5.0",
    "eslint-plugin-vue": "^9.0.0",
    "npm-run-all": "^4.1.5",
    "sass": "^1.54.0",
    "typescript": "~4.7.4",
    "vite": "^3.0.1",
    "vue-tsc": "^0.38.8"
  },
  "dependencies": {
    "@nuxt/kit": "^3.0.0-rc.6",
    "@vue/tsconfig": "^0.1.3",
    "bootstrap": "^5.2.0",
    "bootstrap-icons": "^1.9.1",
    "vue": "^3.2.37",
    "vue-router": "^4.1.2"
  }
}
